{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/Styles/(.*)",
      "dest": "/Public/Styles/$1"
    },
    {
      "src": "/JavaScript/(.*)",
      "dest": "/Public/JavaScript/$1"
    },
    {
      "src": "/Images/(.*)",
      "dest": "/Public/Images/$1"
    },
    {
      "src": "/Audio/(.*)",
      "dest": "/Public/Audio/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
